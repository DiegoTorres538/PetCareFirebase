<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diegotorres.petcarefirebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_petcare"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdateActivity"></activity>
        <activity android:name=".SplashPetCare">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistroPetCare" />
        <activity android:name=".LoginPetCare" />
        <activity android:name=".PetSittActivity" />
        <activity android:name=".DogSittActivity" />
        <activity android:name=".AddPetActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>